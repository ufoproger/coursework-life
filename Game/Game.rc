LANGUAGE LANG_ENGLISH, SUBLANG_ENGLISH_US

IDR_MAINFRAME           ICON                    "res\\Game.ico"

IDR_MAINFRAME MENU
BEGIN
    POPUP "Файл"
    BEGIN
        MENUITEM "Новая игра",                  ID_NEW_GAME
        MENUITEM "Открыть игру",                ID_OPEN_GAME
        MENUITEM "Сохранить игру как...",       ID_SAVE_GAME
        MENUITEM SEPARATOR
        MENUITEM "Выход",                       ID_APP_EXIT
    END
    POPUP "Вид"
    BEGIN
        MENUITEM "Информационная панель",       ID_VIEW_STATUS_BAR
    END
    POPUP "Эволюция"
    BEGIN
        MENUITEM "Каждую секунду",              ID_EVOLVE_1_SECOND
        MENUITEM "Каждые 3 секунды",            ID_EVOLVE_3_SECONDS
        MENUITEM "Каждые 10 секунд",            ID_EVOLVE_10_SECONDS
        MENUITEM "Каждые 42 минуты",            ID_EVOLVE_42_MINUTES
        MENUITEM SEPARATOR
        MENUITEM "Вручную",                     ID_EVOLVE_HAND
    END
    POPUP "Помощь"
    BEGIN
        MENUITEM "О программе...",              ID_APP_ABOUT
    END
END

IDR_MAINFRAME ACCELERATORS
BEGIN
    "C",            ID_EDIT_COPY,           VIRTKEY, CONTROL, NOINVERT
    "V",            ID_EDIT_PASTE,          VIRTKEY, CONTROL, NOINVERT
    VK_BACK,        ID_EDIT_UNDO,           VIRTKEY, ALT, NOINVERT
    VK_DELETE,      ID_EDIT_CUT,            VIRTKEY, SHIFT, NOINVERT
    VK_F6,          ID_NEXT_PANE,           VIRTKEY, NOINVERT
    VK_F6,          ID_PREV_PANE,           VIRTKEY, SHIFT, NOINVERT
    VK_INSERT,      ID_EDIT_COPY,           VIRTKEY, CONTROL, NOINVERT
    VK_INSERT,      ID_EDIT_PASTE,          VIRTKEY, SHIFT, NOINVERT
    "X",            ID_EDIT_CUT,            VIRTKEY, CONTROL, NOINVERT
    "Z",            ID_EDIT_UNDO,           VIRTKEY, CONTROL, NOINVERT
END

IDD_ABOUTBOX DIALOGEX 0, 0, 201, 81
STYLE DS_SETFONT | DS_MODALFRAME | DS_FIXEDSYS | WS_POPUP | WS_CAPTION | WS_SYSMENU
CAPTION "О программе"
FONT 8, "MS Shell Dlg", 0, 0, 0x1
BEGIN
    ICON            IDR_MAINFRAME,IDC_STATIC,14,14,21,20
    LTEXT           "Игра ""Жизнь"", курсовая работа",IDC_STATIC,42,14,114,8,SS_NOPREFIX
    LTEXT           "Студент: Снетков М.С., ИМ11-01Б",IDC_STATIC,42,26,152,8
    DEFPUSHBUTTON   "OK",IDOK,144,60,50,14,WS_GROUP
    LTEXT           "Преподаватель: Баранова И.В.",IDC_STATIC,42,38,152,8
    LTEXT           "ИМ СФУ, 2012",IDC_STATIC,42,50,152,8
END

VS_VERSION_INFO VERSIONINFO
 FILEVERSION 1,0,0,1
 PRODUCTVERSION 1,0,0,1
